SUBDESIGN frequencia_contador_1Hz
(
	clock,enable : INPUT;	
	clock_saida: OUTPUT;
)

VARIABLE
	count[3..0] : DFF;
	flag : NODE;
	
BEGIN
	count[].clk = !clock;
	
	IF enable THEN					
		IF count[].q >= 0 & count[].q <= 3 THEN
			clock_saida = GND;
			count[].d = count[].q + 1;
		ELSIF count[].q >= 4 & count[].q <= 8 THEN
			clock_saida =  VCC;
			count[].d = count[].q + 1;
		ELSE
			count[].d = 0;
		END IF;				
	ELSE
		count[].d = count[].q;
	END IF;

END;